"*":
  autosave:
    enabled: true
  core:
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
    ]
    ignoredNames: [
      ".DS_Store"
      ".git"
      ".hg"
      ".sass-cache"
      ".svn"
      "bower_components"
      "CVS"
      "node_modules"
      "temp"
      "Thumbs.db"
    ]
    themes: [
      "encounters-light-ui"
      "encounters-light-syntax"
    ]
  editor:
    backUpBeforeSaving: true
    fontFamily: "Input Mono"
    fontSize: 12
    invisibles: {}
    showIndentGuide: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    softWrapped: true
  "release-notes":
    viewedVersion: "0.95.0"
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
